{
  "trigger_prefix": {
    "description": "助手前缀",
    "type": "string",
    "hint": "裸命令触发前缀，需“前缀+空格+任务”，例如：助手 帮我诊断",
    "default": "助手"
  },
  "enable_slash_command": {
    "description": "启用斜杠命令",
    "type": "bool",
    "hint": "开启后支持 /助手 ...",
    "default": true
  },
  "enable_plain_prefix_command": {
    "description": "启用裸前缀命令",
    "type": "bool",
    "hint": "开启后支持“助手 任务”触发",
    "default": true
  },
  "admin_user_ids": {
    "description": "插件管理员用户ID",
    "type": "list",
    "hint": "额外管理员（字符串列表）；会与 Astr 全局 admins_id 合并",
    "default": []
  },
  "whitelist_user_ids": {
    "description": "白名单用户ID",
    "type": "list",
    "hint": "允许触发助手的用户ID",
    "default": []
  },
  "whitelist_group_ids": {
    "description": "白名单群ID",
    "type": "list",
    "hint": "仅这些群允许触发（群聊默认不触发）",
    "default": []
  },
  "manage_group_ids": {
    "description": "管理群ID",
    "type": "list",
    "hint": "管理群内可关闭脱敏（仍受权限与高危确认约束）",
    "default": []
  },
  "global_permission_level": {
    "description": "全局权限等级",
    "type": "string",
    "options": [
      "L0",
      "L1",
      "L2",
      "L3",
      "L4"
    ],
    "hint": "L0普通对话；L1只读；L2可执行Astr动作；L3主机非root；L4 root级",
    "default": "L2"
  },
  "high_risk_confirm_enabled": {
    "description": "启用高危确认",
    "type": "bool",
    "hint": "开启后，高危工具操作需先 /助手 确认 <token>",
    "default": true
  },
  "confirm_ttl_seconds": {
    "description": "高危确认有效期（秒）",
    "type": "int",
    "hint": "确认后在该时间窗口内可执行高危操作",
    "default": 300
  },
  "privacy_mask_enabled": {
    "description": "启用隐私脱敏",
    "type": "bool",
    "hint": "输出中脱敏 token/key/secret/password 等字段",
    "default": true
  },
  "privacy_exempt_private": {
    "description": "私聊豁免脱敏",
    "type": "bool",
    "hint": "管理员/白名单私聊时关闭脱敏",
    "default": true
  },
  "privacy_exempt_manage_groups": {
    "description": "管理群豁免脱敏",
    "type": "bool",
    "hint": "manage_group_ids 内群聊关闭脱敏",
    "default": true
  },
  "silent_unauthorized": {
    "description": "未授权静默",
    "type": "bool",
    "hint": "未授权触发是否静默不回复",
    "default": true
  },
  "blacklist_plugins": {
    "description": "插件黑名单",
    "type": "list",
    "hint": "未来用于动作路由拦截（按插件名）",
    "default": []
  },
  "blacklist_commands": {
    "description": "命令黑名单",
    "type": "list",
    "hint": "屏蔽 astr_exec_command 中的命令名",
    "default": []
  },
  "blacklist_tools": {
    "description": "工具黑名单",
    "type": "list",
    "hint": "屏蔽本插件暴露给 OpenClaw 的工具名",
    "default": []
  },
  "blacklist_shell_patterns": {
    "description": "Shell黑名单模式",
    "type": "list",
    "hint": "正则或子串；命中后拒绝 host_exec",
    "default": []
  },
  "gateway_primary_url": {
    "description": "OpenClaw主网关URL",
    "type": "string",
    "hint": "例如 http://127.0.0.1:18789",
    "default": "http://127.0.0.1:18789"
  },
  "gateway_backup_url": {
    "description": "OpenClaw备网关URL",
    "type": "string",
    "hint": "可选；主网关失败时尝试",
    "default": ""
  },
  "gateway_bearer_token": {
    "description": "网关Bearer Token",
    "type": "string",
    "hint": "对应 OpenClaw gateway.auth.token",
    "default": ""
  },
  "gateway_agent_id": {
    "description": "OpenClaw Agent ID",
    "type": "string",
    "hint": "将使用 model=openclaw:<agent_id>",
    "default": "main"
  },
  "request_timeout_seconds": {
    "description": "网关请求超时（秒）",
    "type": "int",
    "hint": "建议 30-120 秒",
    "default": 90
  },
  "max_parallel_turns": {
    "description": "最大并发会话数",
    "type": "int",
    "hint": "兼容保留项；V1 运行时会强制钳制为 1（可在诊断查看）",
    "default": 1
  },
  "tool_call_timeout_seconds": {
    "description": "Astr工具执行超时（秒）",
    "type": "int",
    "hint": "用于 astr_exec_tool 的本地工具调度超时控制，建议 15-120 秒",
    "default": 45
  },
  "executor_priority": {
    "description": "执行器优先级策略",
    "type": "string",
    "options": [
      "codex_then_gemini_then_shell",
      "gemini_then_codex_then_shell",
      "shell_only"
    ],
    "hint": "V1 主要用于诊断展示，后续版本用于执行器调度",
    "default": "codex_then_gemini_then_shell"
  }
}
